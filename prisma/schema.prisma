generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SequelizeMeta {
  name String @id
}

model category {
  id         Int      @id @default(autoincrement())
  name       String   @unique
  created_at DateTime @default(now())
  /// The value of this field is generated by the database as: `CURRENT_TIMESTAMP`.
  updated_at String   @default(dbgenerated())
}

model donation_centers {
  id             Int                                @id @default(autoincrement())
  created_at     DateTime                           @default(now())
  /// The value of this field is generated by the database as: `CURRENT_TIMESTAMP`.
  updated_at     String                             @default(dbgenerated())
  name           String
  city           String
  state          String
  zip            String?
  phone_number   String?
  contact_name   String?
  email          String?
  instagram      String?
  website        String?
  notes          String?
  federal_tax_id String?
  categories     enum_donation_centers_categories[]
  address        String?                            @unique
}

enum enum_donation_centers_categories {
  Apparel
  Hygiene___Body_Care       @map("Hygiene & Body Care")
  Electronics___Appliances  @map("Electronics & Appliances")
  Home_Goods                @map("Home Goods")
  Food___Supplements        @map("Food & Supplements")
  Gift_Cards                @map("Gift Cards")
  Art___School_Supplies     @map("Art & School Supplies")
  Pet_Supplies              @map("Pet Supplies")
  Books
}
